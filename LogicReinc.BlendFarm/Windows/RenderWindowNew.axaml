<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="1920" d:DesignHeight="1080"
        x:Class="LogicReinc.BlendFarm.Windows.RenderWindowNew"
        Title="NewRenderWindow"
		MinWidth="900" MinHeight="700" Width="1920" Height="1080">
	<Grid ColumnDefinitions="*,Auto,Auto" RowDefinitions="Auto,2*,*" Background="{DynamicResource Alt}">

		<!-- Header Bar -->
		<Grid ColumnDefinitions="Auto,Auto">
			<ListBox Background="Transparent" Classes="Projects" Items="{Binding Projects}" SelectionChanged="ProjectTabChanged" SelectedIndex="{Binding TabScrollIndex}">
				<ListBox.ItemsPanel>
					<ItemsPanelTemplate>
						<StackPanel Orientation="Horizontal" />
					</ItemsPanelTemplate>
				</ListBox.ItemsPanel>
				<ListBox.ItemTemplate>
					<DataTemplate>
						<Button Content="{Binding Name}" Command="{Binding SelectProject}"></Button>
					</DataTemplate>
				</ListBox.ItemTemplate>
			</ListBox>
			<Button Grid.Column="2" FontSize="20" Padding="3,0" Margin="5,0" Command="{Binding OpenProjectDialog}">ï¼‹</Button>
		</Grid>
		<Grid ColumnDefinitions="*,*" Grid.Column="1">
			<Button HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" x:Name="GitHub" Command="{Binding Github}">
				<Image Source="/Icons/github.png" Height="15"></Image>
			</Button>
			<Button Grid.Column="1" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" x:Name="Patreon" Command="{Binding Patreon}">
				<Image Source="/Icons/patreon.png" Height="15"></Image>
			</Button>
		</Grid>
		<Button Grid.Column="2" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" x:Name="Help" Command="{Binding Help}">
			Help!
		</Button>

		<Border Grid.Row="1" Grid.RowSpan="2" Grid.ColumnSpan="2" Background="{DynamicResource Main}">
			<Grid RowDefinitions="*,Auto">
				<Image x:Name="render"
            
					  Source="{Binding CurrentProject.LastBitmap}"
					  Grid.Row="0"
					  Grid.Column="0">
				</Image>
				<ProgressBar
					  IsVisible="{Binding CurrentProject.IsRendering}"
					  x:Name="renderProgress"
					  Grid.Column="0"
					  Grid.Row="1">

				</ProgressBar>
			</Grid>
		</Border>
		<!-- Menu Bar -->


		<Grid Grid.Column="2" Grid.Row="1" RowDefinitions="*,Auto,*" Classes="MenuButtons">
			<!-- Menu Buttons -->
			<ListBox Classes="MenuButtons" Grid.Column="0" Grid.Row="1" x:Name="menuOptions">
				<ListBoxItem Classes="MenuButtons" x:Name="Nodes" Padding="0" Margin="0">
					<TextBlock>Nodes</TextBlock>
				</ListBoxItem>
				<ListBoxItem Classes="MenuButtons" x:Name="Settings" Padding="0" Margin="0">
					<TextBlock>Settings</TextBlock>
				</ListBoxItem>
				<ListBoxItem Classes="MenuButtons" x:Name="Queue" Padding="0" Margin="0">
					<TextBlock>Queue</TextBlock>
				</ListBoxItem>
			</ListBox>
		</Grid>
		<Grid Classes="MenuButtons" Grid.Column="2"  RowDefinitions="*,Auto,*" Grid.Row="2">
			<ListBox Classes="MenuButtons" Grid.Row="1" SelectedIndex="0">
				<ListBoxItem Classes="MenuButtons" x:Name="Render">
					<TextBlock>Render</TextBlock>
				</ListBoxItem>
			</ListBox>
		</Grid>
		<!-- Menu Panels -->
		<Grid Grid.Column="1" Grid.Row="1" x:Name="menuPanel" Width="400" RowDefinitions="*">

			<!-- Nodes Setup menu panel -->
			<Border Classes="Menu" Padding="25" IsVisible="{Binding #Nodes.IsSelected}" x:Name="menuNodes">
				<Grid RowDefinitions="Auto,Auto,*, Auto">
					<Grid RowDefinitions="Auto,Auto,Auto,Auto,Auto" ColumnDefinitions="Auto, *">
						<TextBlock Grid.Row="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" Margin="0,0,0,5">Add New Node:</TextBlock>
						<TextBlock Grid.Row="1">Name:</TextBlock>
						<TextBox Grid.Row="1" Grid.Column="1" Watermark="Name of Computer/Node" Margin="10,0,0,5" x:Name="inputClientName" Text="{Binding InputClientName}" />
						<TextBlock Grid.Row="2">Address:</TextBlock>
						<TextBox Grid.Row="2" Grid.Column="1" Watermark="198.168.1.1:15000" Margin="10,0,0,10" x:Name="inputClientAddress" Text="{Binding InputClientAddress}" />
						<Button Content="Connect!" 
                          Command="{Binding AddNewNode}"
                          IsEnabled="{Binding ElementName=inputClientAddress, Converter={x:Static StringConverters.IsNotNullOrEmpty}}"
								Grid.ColumnSpan="2" Grid.Row="4" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Padding="20,5" />
					</Grid>
					<Grid ColumnDefinitions="*,Auto" Grid.Row="1">
						<TextBlock HorizontalAlignment="Center" Margin="0,20,0,5">Render Nodes:</TextBlock>
						<TextBlock Grid.Column="1"></TextBlock>
					</Grid>
					<Border Grid.Row="2" Classes="NodeList">
						<ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Hidden">
							<ItemsControl Items="{Binding Nodes}">
								<ItemsControl.ItemTemplate>
									<DataTemplate>
										<Border Classes="Node">
											<Grid RowDefinitions="Auto, Auto" ColumnDefinitions="2*,*">
												<ToggleButton FontWeight="Normal" FontSize="14" Content="{Binding Name}" IsVisible="{Binding !Connected}"/>
												<ToggleButton FontWeight="Bold" FontSize="14" Content="{Binding Name}" Classes="Connected">
													<ToggleButton.IsVisible>
														<MultiBinding Converter="{x:Static BoolConverters.And}">
															<Binding Path="!IsSynced"></Binding>
															<Binding Path="Connected"></Binding>
														</MultiBinding>
													</ToggleButton.IsVisible>
												</ToggleButton>
												<ToggleButton FontWeight="Bold" FontSize="20" Classes="Connected Synced" Content="{Binding Name}">
													<ToggleButton.IsVisible>
														<MultiBinding Converter="{x:Static BoolConverters.And}">
															<Binding Path="IsSynced"></Binding>
															<Binding Path="Connected"></Binding>
														</MultiBinding>
													</ToggleButton.IsVisible>
												</ToggleButton>
												<TextBlock  Grid.Column="2" HorizontalAlignment="Stretch" TextAlignment="Center" VerticalAlignment="Top" Text="{Binding Address}" />

												<Button Grid.Column="2" Grid.Row="1" Margin="5,0,0,0" IsVisible="{Binding !Connected}"
														IsEnabled="{Binding Activity, Converter={x:Static StringConverters.IsNullOrEmpty}}"
														Command="{Binding ConnectAndPrepare}"
														CommandParameter="{Binding RelativeSource={RelativeSource AncestorType=Window}, Path=DataContext.Version.Name}">
													Connect
												</Button>
												<Button Grid.Column="2" Grid.Row="1" Margin="5,0,0,0" IsVisible="{Binding Connected}" Command="{Binding Disconnect}">Disconnect</Button>
												<Border Height="27" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2" >
													<TextBlock VerticalAlignment="Center" TextWrapping="Wrap" Text="{Binding Activity}" ToolTip.Tip="{Binding Activity}" />
												</Border>
											</Grid>
										</Border>
									</DataTemplate>
								</ItemsControl.ItemTemplate>
							</ItemsControl>
